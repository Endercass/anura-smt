{
  "compilerOptions": {
    "allowJs": true,
    "allowUnreachableCode": false,
    "alwaysStrict": true,
    "checkJs": true,
    "declaration": true,
    "esModuleInterop": true,
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "YOU_CANT_USE_FRAGMENTS",
    "module": "ESNext",
    "lib": ["ESNext", "dom"],
    "moduleResolution": "Bundler",
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "outDir": "./build/",
    "rootDir": "./src/",
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "ESNext",
    "types": ["@mercuryworkshop/alicejs"]
  },
  "references": [
    {
      "path": "./src/app"
    },
    {
      "path": "./src/lib"
    }
  ],
  "include": []
}
